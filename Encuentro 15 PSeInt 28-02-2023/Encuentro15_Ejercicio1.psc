	Leer a;
	
	Escribir "Por favor ingrese un valor entero para B: ";
	Leer b;	
	
	interc(a,b)
	
FinAlgoritmo

SubProceso interc(a,b)
	
	Escribir "El primer valor sera: ",b " Y el segundo valor sera: " ,a;
	
FinSubProceso
