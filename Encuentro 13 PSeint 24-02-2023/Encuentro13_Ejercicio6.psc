Algoritmo Encuentro13_Ejercicio6
	
	Definir num Como Entero;
	
	Escribir "Escriba un numero";
	Leer num;
	
	Escribir "La suma de todos los divisores del nummero " num " es " suma(num);
	
FinAlgoritmo


Funcion sumadiv <- suma (num)
	
	Definir sumadiv, i Como Entero;
	
	sumadiv=0;
	
	Para i=1 Hasta num Con Paso 1 Hacer
		
		si num mod i == 0 Entonces
			
			sumadiv= sumadiv+i;
			
		FinSi
		
	FinPara
	
	
FinFuncion
