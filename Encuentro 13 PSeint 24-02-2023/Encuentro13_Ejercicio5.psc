Algoritmo Encuentro13_Ejercicio5
	
	Definir num como entero;
	
	Escribir "Por favor escriba un número";
	Leer num
	
	Escribir "Su numero es primo --->"	primo(num)
	
	
FinAlgoritmo


Funcion esprimo <- primo (num Por Referencia)
	
	Definir esprimo Como Logico;
	
	Definir i, resto,contador Como Entero;
	
	contador=0;
	
	Para i=1 Hasta num Con Paso 1 Hacer
		
		resto=num mod i;
		
		si resto ==0 Entonces
			
			contador=contador+1;
			
		FinSi
		
	FinPara
	
	si contador==2 Entonces
		
		esprimo=Verdadero;
		
	FinSi
	
FinFuncion
