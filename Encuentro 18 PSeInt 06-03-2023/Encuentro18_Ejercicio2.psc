Algoritmo Encuentro18_Ejercicio2
	
//	2. Realizar un programa que lea 10 números reales por teclado, los almacene en un arreglo y
//	muestre por pantalla la suma, resta y multiplicación de todos los números ingresados al arreglo.
	
	Definir Vector, I Como Real;
	
	Escribir "Por favor ingrese 5 valores para el array";
	
	Dimension Vector(10)
	
	Para I=0 Hasta 9 Con paso 1 Hacer
		
		Leer Vector(I)
		
	FinPara
	
	Suma=Vector(9)+Vector(8)+Vector(7)+Vector(6)+Vector(5)+Vector(4)+Vector(3)+Vector(2)+Vector(1)+Vector(0);
	Resta=Vector(9)-Vector(8)-Vector(7)-Vector(6)-Vector(5)-Vector(4)-Vector(3)-Vector(2)-Vector(1)-Vector(0);
	Multiplicacion=Vector(9)*Vector(8)*Vector(7)*Vector(6)*Vector(5)*Vector(4)*Vector(3)*Vector(2)*Vector(1)*Vector(0);
	
	Escribir "la suma de los vectores ingresado es: ",Suma;
	
	Escribir "la resta de los vectores ingresado es: ",Resta;
	
	Escribir "la multiplicación de los vectores ingresado es: ",Multiplicacion;
	
